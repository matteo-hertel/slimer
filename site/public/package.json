{
  "name": "slimer",
  "version": "1.0.0",
  "description": "Slimer theme for hugo",
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist/ && webpack --mode production --config webpack.config.js",
    "dev": "webpack-dev-server --mode development --config webpack.config.js",
    "lint:js": "eslint . --ext js,jsx",
    "lint:js:fix": "eslint . --ext js,jsx --fix",
    "preinstall": "npx Skyscanner/ensure-node-env",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Matteo Hertel <info@matteohertel.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-skyscanner-with-prettier": "^0.4.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
