{
  "name": "slimer",
  "version": "1.0.0",
  "description": "Slimer theme for hugo",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "main": "src/index.js",
  "collection": "build/components/collection/collection-manifest.json",
  "types": "build/components/types/components.d.ts",
  "module": "build/components/esm/es5/index.js",
  "scripts": {
    "build": "rm -rf ./dist/ && webpack --mode production --config webpack.prod.config.js",
    "clean": "rm -rf dist build dist .stencil",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "components:build": "stencil build --docs",
    "components:start": "stencil build --dev --watch --serve",
    "components:test": "stencil test --spec --e2e",
    "components:test.watch": "stencil test --spec --e2e --watchAll",
    "dev": "webpack-dev-server --mode development --config webpack.dev.config.js",
    "lint:js": "eslint . --ext js,jsx",
    "lint:js:fix": "eslint . --ext js,jsx --fix",
    "postbuild": "hugo -d ../dist -s site",
    "prebuild": "npm run components:build",
    "precomponents:build": "rm -rf build/components",
    "predev": "npm run components:build",
    "preinstall": "npx Skyscanner/ensure-node-env",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\"",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Matteo Hertel <info@matteohertel.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@stencil/core": "~0.15.2",
    "@stencil/webpack": "0.0.6",
    "@storybook/html": "^4.1.6",
    "@types/jest": "23.3.12",
    "@types/puppeteer": "1.11.1",
    "acorn": "^6.0.4",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "eslint-config-skyscanner-with-prettier": "^0.4.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "puppeteer": "1.8.0",
    "sass-loader": "^7.1.0",
    "svg-url-loader": "^2.3.2",
    "typescript-eslint-parser": "^21.0.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
